<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix = "fmt" uri = "http://java.sun.com/jsp/jstl/fmt" %>
<%@ page isELIgnored="false" %>
			<div role="tabpanel" class="tab-pane fade" id="home2" aria-labelledby="home2-tab">
						  <div class="w3l_stocks">
							  <c:forEach items="${listStocksData}" var="listStocksDataVar">
                                 <div class="w3l_stocks1">
                                    <a href="https://p.w3layouts.com/demos/sep-2016/15-09-2016/trade_market/web/single.html">
                                       <h4>${listStocksDataVar.companyName}</h4>
                                    </a>
                                    <p>${listStocksDataVar.price}<i style="color:#00AA00;"><span class="caret caret1"></span>${listStocksDataVar.value}<label>(0.25%)</label></i></p>
                                 </div>
								 </c:forEach>
                           </div>
						 </div> 
			<div role="tabpanel" class="tab-pane fade" id="latest2" aria-labelledby="latest2-tab">			 
						   <div class="w3l_stocks">
							 <c:forEach items="${listFundsData}" var="listFundsDataVar">
                                 <div class="w3l_stocks1">
                                    <a href="https://p.w3layouts.com/demos/sep-2016/15-09-2016/trade_market/web/single.html">
                                       <h4>${listFundsDataVar.companyName}</h4>
                                    </a>
                                    <p>${listFundsDataVar.price}<i style="color:#00AA00;"><span class="caret caret1"></span>${listFundsDataVar.value}<label>(0.33%)</label></i></p>
                                 </div>
							</c:forEach>
                              </div>
					</div>	  
            <div role="tabpanel" class="tab-pane fade active in" id="experts2" aria-labelledby="experts2-tab">               
                              <div class="w3l_stocks">
							  <c:forEach items="${listCommoditiesData}" var="listCommoditiesDataVar">
                                 <div class="w3l_stocks1">
                                    <a href="https://p.w3layouts.com/demos/sep-2016/15-09-2016/trade_market/web/single.html">
                                       <h4 style="text-transform:capitalize;">${listCommoditiesDataVar.companyName}</h4>
                                    </a>
                                    <p>${listCommoditiesDataVar.price}<i><span class="caret"></span>${listCommoditiesDataVar.value}<label>(-1.26%)</label></i></p>
                                 </div>
                                </c:forEach> 
                           </div>
						</div>
					
						   
                        
